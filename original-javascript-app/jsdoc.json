{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["scripts"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false
    }
  },
  "opts": {
    "destination": "./dist/docs/",
    "recurse": true,
    "readme": "./README.md"
  }
}
